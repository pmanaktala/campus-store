  "description": "campus shopping cart",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "build": "cd frontend && npm run build",
    "server": "npx kill-port 4000 && nodemon backend/server # Killing port 4000 before starting server",
